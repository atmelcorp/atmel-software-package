ifeq ($(CONFIG_HAVE_LED),y)
	ifeq ($(CONFIG_LED),y)
		CFLAGS_DEFS += -DCONFIG_HAVE_LED
	else
		CONFIG_HAVE_LED=n
	endif
endif
ifeq ($(CONFIG_HAVE_CONSOLE_DBGU),y)
	CFLAGS_DEFS += -DCONFIG_HAVE_CONSOLE_DBGU
endif
ifeq ($(CONFIG_HAVE_CONSOLE_UART),y)
	CFLAGS_DEFS += -DCONFIG_HAVE_CONSOLE_UART
endif
ifeq ($(CONFIG_HAVE_CONSOLE_USART),y)
	CFLAGS_DEFS += -DCONFIG_HAVE_CONSOLE_USART
endif
ifeq ($(CONFIG_HAVE_FAULT_DEBUG),y)
	CFLAGS_DEFS += -DCONFIG_HAVE_FAULT_DEBUG
endif
ifeq ($(CONFIG_HAVE_UDPHS),y)
	ifeq ($(CONFIG_USB),y)
		CFLAGS_DEFS += -DCONFIG_HAVE_UDPHS
	else
		CONFIG_HAVE_UDPHS=n
		CONFIG_LIB_USB=n
		CONFIG_LIB_USB_AUDIO=n
		CONFIG_LIB_USB_CDC=n
		CONFIG_LIB_USB_COMPOSITE=n
		CONFIG_LIB_USB_HID=n
		CONFIG_LIB_USB_MSD=n
		CONFIG_LIB_USB_UVC=n
	endif
else
	CONFIG_LIB_USB=n
	CONFIG_LIB_USB_AUDIO=n
	CONFIG_LIB_USB_CDC=n
	CONFIG_LIB_USB_COMPOSITE=n
	CONFIG_LIB_USB_HID=n
	CONFIG_LIB_USB_MSD=n
	CONFIG_LIB_USB_UVC=n
endif
